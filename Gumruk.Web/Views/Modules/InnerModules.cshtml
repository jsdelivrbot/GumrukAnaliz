@model List<Gumruk.Entity.InnerModule>

<div class="row padding-left5">
    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3 pull-left">
        <strong>Module Name</strong>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
        <strong>FileName</strong>
    </div>
    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
        <strong>Calling FileName</strong>
    </div>
    <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1 pull-right">
        <strong>Method</strong>
    </div>
</div>
@foreach (var item in Model)
{
    <div class="row padding-left5" style="border-bottom:1px solid #f6f6f6;padding-bottom:3px;">
        <div class="col-lg-3 col-md-3 col-sm-3 col-xs-3 pull-left">
            @item.Module.Name
        </div>
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
            @item.ContainFileName
        </div>
        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
            @item.ModFileName
        </div>
        <div class="col-lg-1 col-md-1 col-sm-1 col-xs-1 pull-right">
            <a href="javascript:void(0)" data-toggle="collapse" data-target="#method_@item.ID"><span class="fa fa-archive"></span></a>
        </div>
    </div>
    <div   id="method_@item.ID" class="collapse" style="border-bottom:1px solid #f6f6f6;padding-bottom:3px;background-color:#e4fffd;color:#015dce;">
        @item.Method
    </div>
}






