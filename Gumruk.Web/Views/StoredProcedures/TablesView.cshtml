@model ICollection<Gumruk.Entity.d_b_tables>

<div class="table-responsive" style="background-color:#0094ff;color:#fff; max-height:400px;overflow-y:auto;padding-left:25px;overflow-x:hidden;">

    <div class="row">
        <div class="col-lg-3">
            <strong>Name</strong>
        </div>
        <div class="col-lg-9 ">
            <strong>Açıklamalar</strong>
</div>

    </div>

    @foreach (var itemTable in Model)
            {
        <div class="row">
            <div class="col-lg-2">
                @itemTable.name
            </div>
            <div class="col-lg-10">
                @foreach (var item2 in itemTable.d_b_table_explanation)
                {
                    @item2.details<br />
                }
            </div>
        </div>
    }


</div>







