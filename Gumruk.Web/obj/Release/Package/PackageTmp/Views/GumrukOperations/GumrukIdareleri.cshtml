@model List<Gumruk.Entity.GumrukIdareleri>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Kod</th>
            <th colspan="2">Gümrük İdaresi</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td><span id="Kod_@item.ID">@item.ID</span></td>
                <td>
                    <span>@item.GumrukIdaresiAdi</span>
                </td>
                <td><button id="sec" class="btn btn-primary" onclick="GumrukIdaresiSecildi(@item.ID,'@item.GumrukIdaresiAdi')">Seç</button></td>
            </tr>
        }
    </tbody>
</table>
<div class="modal-footer">
    <button type="button" class="btn btn-default" data-dismiss="modal">Kapat</button>
</div>
<input type="hidden" id="beyanTurKodu" />

<script>
    function GumrukIdaresiSecildi(ID, idare) {

        $("#txtIlkVarisCikisYeri").val(ID);
        

        $("#myModal").modal('hide');
    }
</script>



